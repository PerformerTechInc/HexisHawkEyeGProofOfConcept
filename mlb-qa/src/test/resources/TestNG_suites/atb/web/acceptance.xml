<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite verbose="1" name="MLB - At the Ballpark Web - Acceptance" skipfailedinvocationcounts="false" junit="false" parallel="methods" data-provider-thread-count="1"   
	annotations="JDK">
